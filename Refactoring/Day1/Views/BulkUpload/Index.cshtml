@model ViewModel.FileUploadViewModel

@{
    Layout = "~/Views/Shared/_LayoutWithModel.cshtml";
}

@section Title {
    Upload File
}

@section ContentBody {
    <div class="row form-group">
        <div class="col-md-12">
            <h2>Import Employees from CSV File</h2>
            <a href="/Employee/Index" class="btn btn-default">Back</a>
        </div>
    </div>
    <form action="/BulkUpload/Upload" method="post" enctype="multipart/form-data">
        <div class="form-group">
            <label for="fileUpload">Select file</label>
            <input type="file" name="fileUpload" id="fileUpload" value="" class="form-control" />
        </div>
        <input type="submit" name="BtnSubmit" value="Upload" class="form-control btn btn-primary" />
    </form>
}
